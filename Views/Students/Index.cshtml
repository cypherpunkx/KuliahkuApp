@model IEnumerable<KuliahkuApp.Models.Student>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nama_Mhs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tgl_Lahir)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Alamat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JenisKelamin)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nama_Mhs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tgl_Lahir)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Alamat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JenisKelamin)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Nim">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Nim">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Nim">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
